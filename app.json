{
  "expo": {
    "name": "Blog App",
    "slug": "rn-blog-app",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/rn-icon.png",
    "splash": {
      "image": "./assets/rn-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#242833"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dess-dess-dev.blog-app",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.dess_dess_dev.blog_app",
      "permissions": ["CAMERA", "WRITE_EXTERNAL_STORAGE"]
    }
  }
}
